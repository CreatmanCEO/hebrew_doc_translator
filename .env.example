# Server Configuration
PORT=3000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
TRANSLATION_CACHE_TTL=604800  # 7 days in seconds

# OpenAI Configuration
OPENAI_API_KEYS=key1,key2,key3  # Список ключей через запятую
OPENAI_ORG_IDS=org1,org2  # Список ID организаций через запятую

# Google Configuration
GOOGLE_GEMINI_KEY=your_gemini_key
GOOGLE_MAPS_KEYS=key1,key2  # Список ключей через запятую

# Huggingface Configuration
HUGGINGFACE_TOKEN=your_huggingface_token

# Other APIs
JUDGE0_API_KEY=your_judge0_key
HUME_API_KEY=your_hume_key
RAPID_API_KEY=your_rapid_api_key

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=10

# Translation Options
BATCH_SIZE=5  # Количество блоков для одновременного перевода

# Logging Configuration
LOG_LEVEL=info  # debug, info, warn, error
LOG_MAX_SIZE=10485760  # 10MB
LOG_MAX_FILES=5
LOG_FORMAT=json  # json, simple
LOG_CONSOLE_ENABLED=true  # Включить вывод в консоль
LOG_FILE_ENABLED=true  # Включить запись в файл
LOG_DIR=logs  # Директория для файлов логов 